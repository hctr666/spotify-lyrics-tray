{
  "name": "spotify-lyrics-tray-app",
  "version": "1.0.0",
  "description": "A minimalist Spotify lyrics tray app",
  "private": true,
  "main": "./src/main/bootstrap.js",
  "scripts": {
    "package": "NODE_ENV=production electron-forge package",
    "start": "NODE_ENV=development concurrently -k \"yarn renderer:start\" \"yarn main:start\"",
    "main:start": "wait-on -i 250 -w 250 -d 2500 http://localhost:4014 && electron-forge start",
    "renderer:start": "vite",
    "renderer:build": "yarn typecheck && vite build",
    "renderer:test": "jest --runInBand true",
    "lint": "eslint .",
    "prepare": "husky install",
    "typecheck": "tsc --project ./src/renderer/app/tsconfig.json"
  },
